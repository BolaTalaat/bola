<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.example.bolatalaat.itsharks">

    <uses-permission android:name="android.permission.CALL_PHONE" />
    <uses-permission android:name="android.permission.INTERNET" />

    <application
        android:allowBackup="true"
        android:icon="@drawable/itsharks"
        android:label="@string/app_name"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">
        <activity android:name="com.example.bolatalaat.itsharks.MainActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity android:name="com.example.bolatalaat.itsharks.home" />
        <activity android:name="com.example.bolatalaat.itsharks.Instructors" />
        <activity android:name="com.example.bolatalaat.itsharks.Reviews" />
        <activity android:name="com.example.bolatalaat.itsharks.Contacts" />
        <activity android:name="com.example.bolatalaat.itsharks.Courses" />
        <activity android:name="com.example.bolatalaat.itsharks.Course_data"></activity>
    </application>

</manifest>